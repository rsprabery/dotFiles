  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.jediEnabled": false,
  "editor.fontFamily": "SFMono-semibold,Menlo, Monaco, 'Courier New', monospace",
  "python.analysis.diagnosticPublishDelay": 3000,
  "python.analysis.logLevel": "Trace",
  "python.analysis.memory.keepLibraryAst": true,
  "python.analysis.memory.keepLibraryLocalVariables": true,
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": ["-l 79"],
  "python.venvFolders": ["~/workspace/virtenvs"],
  "editor.rulers": [80, 120],
  "vim.easymotion": true,
  "vim.leader": ",",
  "vim.easymotionMarkerFontSize": "16",
  "vim.easymotionMarkerFontWeight": "semi-bold",
  "vim.easymotionMarkerHeight": 18,
  "vim.easymotionMarkerWidthPerChar": 10,
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "~/.vscode/vimrc",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.quickOpenLeastRecentlyUsedEditor"]
    },
    {
      "before": ["<leader>", "g"],
      "commands": ["workbench.action.files.copyPathOfActiveFile"]
    },
    {
      "before": ["<leader>", "v"],
      "commands": ["workbench.action.splitEditorRight"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["workbench.action.splitEditorDown"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.quickOpenView"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["outline.focus"]
    },
    {
      "before": ["<leader>", "n"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["<leader>", "<leader>", "c"],
      "commands": ["vscode-coverage-highlighter.toggle"]
    }
  ],
  "macros": {
    "saveAndExitInsertMode": [
      "extension.vim_escape",
      "workbench.action.files.save"
    ]
  },
  "editor.minimap.enabled": false,
  "workbench.sideBar.location": "left",
  "vim.useSystemClipboard": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "terminal.integrated.fontFamily": "\"Roboto Mono Medium for Powerline\",SFMono-semibold,Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.macOptionIsMeta": true,
  "go.useLanguageServer": true,
  "python.languageServer": "Default",
  "search.useGlobalIgnoreFiles": true,
  "go.toolsManagement.autoUpdate": true,
  "C_Cpp.intelliSenseEngine": "Disabled",
  "javascript.format.enable": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "window.zoomLevel": 1,
  "docker.contexts.label": "DockerEndpoint",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "terminal.integrated.env.osx": {},
  "console-ninja.featureSet": "Community",
  "editor.formatOnSave": true,
  "[ruby]": {
    "editor.defaultFormatter": "Shopify.ruby-lsp",
    "editor.formatOnType": true,
    "editor.insertSpaces": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.semanticHighlighting.enabled": true,
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "files.trimTrailingWhitespace": true,
    "editor.rulers": [
      120
    ]
  },
  "redhat.telemetry.enabled": false,
  "editor.inlineSuggest.enabled": true,
  "terminal.integrated.env.linux": {},
  "typescript.format.enable": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.linkedEditing": true
}
