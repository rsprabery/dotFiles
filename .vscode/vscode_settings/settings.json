    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.colorTheme": "Visual Studio Dark",
    "python.jediEnabled": false,
    "editor.fontFamily": "SFMono-semibold,Menlo, Monaco, 'Courier New', monospace",
    "python.analysis.diagnosticPublishDelay": 3000,
    "python.analysis.logLevel": "Trace",
    "python.analysis.memory.keepLibraryAst": true,
    "python.analysis.memory.keepLibraryLocalVariables": true,
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "-l 79"
    ],
    "python.venvFolders": [
        "~/workspace/virtenvs"
    ],
    "editor.rulers": [
        80,
        120
    ],
    "vim.easymotion": true,
    "vim.leader": ",",
    "vim.easymotionMarkerFontSize": "16",
    "vim.easymotionMarkerFontWeight": "semi-bold",
    "vim.easymotionMarkerHeight": 18,
    "vim.easymotionMarkerWidthPerChar": 10,
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "~/.vscode/vimrc",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "t"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<leader>",
                "h"
            ],
            "commands": [
                "workbench.action.quickOpenLeastRecentlyUsedEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "g"
            ],
            "commands": [
                "workbench.action.files.copyPathOfActiveFile"
            ]
        },
        {
            "before": [
                "<leader>",
                "v"
            ],
            "commands": [
                "workbench.action.splitEditorRight"
            ]
        },
        {
            "before": [
                "<leader>",
                "s"
            ],
            "commands": [
                "workbench.action.splitEditorDown"
            ]
        },
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                "workbench.action.quickOpenView"
            ]
        },
        {
            "before": [
                "<leader>",
                "o"
            ],
            "commands": [
                "outline.focus"
            ]
        },
        {
            "before": [
                "<leader>",
                "n"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "editor.action.goToReferences"
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>",
                "c"
            ],
            "commands": [
                "vscode-coverage-highlighter.toggle"
            ]
        },
    ],
    "macros": {
        "saveAndExitInsertMode": [
            "extension.vim_escape",
            "workbench.action.files.save"
        ]
    },
    "editor.minimap.enabled": false,
    "workbench.sideBar.location": "left",
    "vim.useSystemClipboard": true,
    "window.zoomLevel": 0,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true
}